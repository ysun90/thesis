@ARTICLE{Melnikov_PPCF_15_QCmodes_compasss,
  author = {A V Melnikov and T Markovic and L G Eliseev and J Adámek and M Aftanas
	and P Bilkova and P Boehm and M Gryaznevich and M Imrisek and S E
